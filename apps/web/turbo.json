{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["./dist/**"]
    },
    "dev": {
      "persistent": true
    },
    "ui:add": {
      "persistent": true,
      "interactive": true,
      "cache": false
    }
  }
}
